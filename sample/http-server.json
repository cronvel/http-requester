{
	"server": true ,
	"protocol": "http" ,
	"port": 8080 ,
	"queryParser": {
		"brackets": true ,
		"autoPush": true ,
		"keyPath": true ,
		"restQueryFilter": "filter" ,
		"autoNumber": true
	} ,
	"responses": [
		{
			"match": {
				"path": "/toto"
			} ,
			"status": 200 ,
			"headers": {
			} ,
			"body": "Toto!"
		} ,
		{
			"match": {
				"host": "localhost"
			} ,
			"status": 200 ,
			"headers": {
			} ,
			"body": "Localhost!"
		} ,
		{
			"match": {
				"host": "127.0.0.1"
			} ,
			"status": 200 ,
			"headers": {
			} ,
			"body": "There is no place like 127.0.0.1"
		}
	] ,
	"default-response": {
		"status": 404 ,
		"headers": {
		} ,
		"body": "404: Not Found -_-'"
	}
}
